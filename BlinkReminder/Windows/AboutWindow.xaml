<Window x:Class="BlinkReminder.Windows.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlinkReminder.Windows"
        mc:Ignorable="d"
        Title="About" x:Name="AboutView" Height="100" Width="200" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Background="#FFF8F8F8">
    <StackPanel Orientation="Vertical">
        <Label x:Name="versionLabel" Margin="0 5 0 3" FontSize="13"/>
        <Label x:Name="updateLabel" Margin="0 0 0 3" FontSize="13"/>
    </StackPanel>
</Window>
